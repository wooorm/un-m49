{
  "name": "un-m49",
  "version": "1.0.0",
  "description": "UN M49 (Standard Country or Area Codes for Statistical Use) codes",
  "license": "MIT",
  "keywords": [
    "un-m49",
    "un",
    "m49",
    "49",
    "country",
    "area",
    "code",
    "name",
    "information"
  ],
  "repository": "wooorm/un-m49",
  "bugs": "https://github.com/wooorm/un-m49/issues",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "main": "index.json",
  "files": [
    "index.json",
    "to-iso-3166.json"
  ],
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "hast-util-select": "^4.0.0",
    "hast-util-to-string": "^1.0.0",
    "prettier": "^2.0.0",
    "rehype-parse": "^7.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "unified": "^9.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "generate": "node build",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node test",
    "test": "npm run format && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prefer-set-has": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
